.acquire();\n  }\n\n  async doQueryAll(\n    connection?: Connection,\n    condition?: string\n  ): Promise<any[] | undefined> {\n    if (connection === undefined) {\n      connection = await this.AcquireDBConnection();\n    }\n    let sqlStatement = `select * from ${this.tableName}`;\n    if (condition !== undefined) {\n      sqlStatement += \" where \" + condition;\n    }\n    const result = await connection.query(sqlStatement);\n    const rows = result.rows;\n    await connection.close(); // Disconnect\n    return rows;\n  }\n}\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"c6aaf8d02a8fc1f05fa8d9c3c087bccf86d4f5c7","contentHash":"46a4a1090f9968be771c84ed73d2dcf1151e9f367048bbf0a83a1ceb2f54527e"}}